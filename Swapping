#include<stdio.h>

int main()
{
      int a, b;
      a=12;
      b=7;
      
      printf("The values of a and b before swapping are: %d and %d", a, b);
      
      a=a+b;
      b=a-b;
      a=a-b;
      
      printf("\nThe values of a and b after swapping are: %d and %d", a, b);
      
      return 0;
}
